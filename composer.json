{
    "name": "hereldar/coding-style",
    "type": "library",
    "description": "PHP Coding Style rules for my projects.",
    "keywords": ["Code style"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hereldar/coding-style/issues",
        "source": "https://github.com/hereldar/coding-style"
    },
    "authors": [
        {
            "name": "Samuel Maudo",
            "email": "samuelmaudo@gmail.com",
            "homepage": "https://github.com/samuelmaudo"
        }
    ],
    "require": {
        "symplify/easy-coding-standard": "^12.1"
    },
    "autoload": {
        "psr-4": {
            "Hereldar\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
